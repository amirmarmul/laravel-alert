{
    "name": "kcdev/laravel-alert",
    "description": "a lightweight package to send alert messages",
    "keywords": [
        "kcdev",
        "laravel-alert"
    ],
    "homepage": "https://github.com/amirmarmul/laravel-alert",
    "license": "MIT",
    "authors": [
        {
            "name": "amirmarmul",
            "email": "amiruddinmarmul@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "illuminate/http": "^5.8|^6.0",
        "illuminate/contracts": "^5.8|^6.0",
        "illuminate/session": "^5.8|^6.0",
        "illuminate/support": "^5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kcdev\\Alert\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kcdev\\Alert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Kcdev\\Alert\\Facades\\Alert"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
